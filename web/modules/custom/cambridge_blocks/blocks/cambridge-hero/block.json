{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "cambridge-blocks/cambridge-hero",
  "title": "Cambridge Hero",
  "category": "media",
  "icon": "cover-image",
  "description": "A hero section with a background image and title.",
  "attributes": {
    "backgroundImage": {
      "type": "object",
      "default": {}
    },
    "imageHeight": {
      "type": "number",
      "default": 400
    }
  },
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "fontFamily": true
    },
    "color": {
      "text": true,
      "background": true,
      "link": true,
      "gradients": true
    }
  },
  "textdomain": "cambridge-hero",
  "editorScript": "file:./index.js"
}