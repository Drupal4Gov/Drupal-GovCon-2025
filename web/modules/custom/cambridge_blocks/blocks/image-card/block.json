{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "cambridge-blocks/image-card",
  "title": "Image Card",
  "category": "layout",
  "icon": "format-image",
  "description": "A card with a background image and a link styled as a button.",
  "attributes": {
    "backgroundImage": {
      "type": "string",
      "default": ""
    },
    "buttonText": {
      "type": "string",
      "default": "Learn More"
    },
    "buttonLink": {
      "type": "string",
      "default": "#"
    }
  },
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    }
  },
  "textdomain": "cambridge-blocks",
  "editorScript": "file:./index.js"
}