<div class="session_archive">
  <div class="session_archive-container container">
    <div class="session_archive-container-page_title session_archive-title">
      <h1 class="session_page_title typography__heading">
        <span>{{ session_page_title.value }}</span>
      </h1>
    </div>
    <div class="session_archive-container-presenters session_archive-presenters">
      <span>{{ session_presenters.label }} {{ session_presenters.value }}</span>    
    </div>
    {% if session_video.value %}
      <div class="session_archive-container-video session_archive-video">
        {{ session_video.value | raw }}
      </div>
    {% endif %}
    <div class="session_archive-container-description session_archive-description body-text">
      <span>{{ session_description.value }}</span>    
    </div>
    <div class="session_archive-container-audience session_archive-audience">
      <fieldset class="session_group">
        <legend class="session_label">
          <span>AUDIENCE</span>
        </legend>
        <div class="session_group-content">
          {% if session_experience_level.label %}
            <div class="session_group-content-item">
              <span class="session_group-content_label">{{ session_experience_level.label }}</span> 
              {{ session_experience_level.value }} 
            </div>
          {% endif %}     
        </div>
      </fieldset> 
    </div>
    <div class="session_archive-container-schedule session_archive-schedule">
      <fieldset class="session_group">
        <legend class="session_label">
          <span>SCHEDULE INFO</span>
        </legend>
        <div class="session_group-content">
          <div class="session_group-content">
            {% if session_conference_year.label %}
              <div class="session_group-content-item">
                <span class="session_group-content_label">{{ session_conference_year.label }}</span> 
                {{ session_conference_year.value }}   
              </div>
            {% endif %}    
          </div>
          <div class="session_group-content">
            {% if session_date_time.label %}
              <div class="session_group-content-item">
                <span class="session_group-content_label">{{ session_date_time.label }}</span>
                {{ session_date_time.value }}   
              </div>
            {% endif %}     
          </div>
          <div class="session_group-content">
            {% if session_room.label %}
              <div class="session_group-content-item">
                <span class="session_group-content_label">{{ session_room.label }}</span>
                {{ session_room.value }}
              </div>
            {% endif %}      
          </div>
        </div>
      </fieldset> 
    </div>
    <div class="session_archive-container-other-info session_archive-other_info">
      <div class="session_archive-other_info-status session_group-content">
        <div class="session_group-content-item">
          <span class="session_group-content_label">{{ session_status.label }}</span>
          {{ session_status.value }}
        </div>
      </div>
      <div class="session_archive-other_info-session_track session_group-content">
        <div class="session_group-content-item">
          <span class="session_group-content_label">{{ session_track.label }}</span>
          {{ session_track.value }}
        </div>
      </div>
    </div>
  </div>
</div>
